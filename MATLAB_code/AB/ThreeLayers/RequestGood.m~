function ordersArray = RequestGood(customersDemand, probabilities)
% InitialiseProbabilities 
ordersArray = zeros(size(probabilities));
iterations = length(customersDemand);
randOrder = randperm(iterations);

spinningWheel = cumsum(probablities,1);
spinningWheel = spinningWheel ./ repmat(spinningWheel(end,:), size(probabilites,1) , 1 );
spins = rand(size(probabilites,2));
for k = 1:iterations
    index_S = find(random < spinning_wheel(:,randOrder(k)),1);
    ordersArray(index_S, randOrder(k)) = customersDemand();
end
end